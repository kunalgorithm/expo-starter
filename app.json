    "orientation": "portrait",
    "icon": "./assets/images/ms_splash.png",
    "scheme": "myapp",
    "backgroundColor": "#B6999A00",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/mindstreaks_name.png",
      "resizeMode": "contain",
