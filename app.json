    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.expostarter.expostarter",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.expostarter.expostarter",
      "versionCode": 1
    }
